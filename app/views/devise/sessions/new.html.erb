<% if params[:no_content_cell] %>
  <div class="form-stacked">
    <%= render 'form' %>
  </div>
<% else %>
  <% if flash[:alert] %>
    <p class="alert"><%= alert %></p>
  <% end %>
  <section class="viewblock no">
  	<header>
  		<h1>Sign In</h1>
  	</header>
  	<div class="viewblock-cont">
  		<article>
  			<%= render 'form' %>
  		</article>
  		
  	</div>
  </section>
<% end %>

