<% form_for ([submission.feed, submission]) do |f| %>
  <%= f.label :moderation_flag, "Approve:" %> <%= f.check_box :moderation_flag %>
  <%= f.label :duration %> <%= f.text_field :duration %>
  <%= f.label :moderator %> <%= f.collection_select :moderator_id, User.all, :id, :name %>
  <%= f.submit %>
<% end %>
