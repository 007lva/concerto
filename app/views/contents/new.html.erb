<div class="MS-frame">
	<h1>Select Type</h1>
	<div class="MS-outer">
		<div class="MS-inner">
			<ul class="iconlist">
				<li> 
			  		<a href=""> 
			  			<div class="iconlist_padding"> 
		        			<div class="ins"> 
		          				<div class="type-folder"><%= image_tag("ph/ph.png", :alt => "") %></div>
		        			</div> 
		        			<h2>Graphics</h2> 
		      			</div> 
			  		</a> 
			  	</li>
				<li> 
			  		<a href=""> 
			  			<div class="iconlist_padding"> 
		        			<div class="ins"> 
		          				<div class="type-folder"><%= image_tag("ph/ph.png", :alt => "") %></div>
		        			</div> 
		        			<h2>Ticker</h2> 
		      			</div> 
			  		</a> 
			  	</li>
				<li> 
			  		<a href=""> 
			  			<div class="iconlist_padding"> 
		        			<div class="ins"> 
		          				<div class="type-folder"><%= image_tag("ph/ph.png", :alt => "") %></div>
		        			</div> 
		        			<h2>Dynamic</h2> 
		      			</div> 
			  		</a> 
			  	</li>
			</ul>
			<div class="clear"></div>
		</div>
	</div>
</div>

<div class="C-full lg">
	<div class="C-header">
		<div class="C-header_back"><a class="button back" href="#" onclick="this.blur();"><span>Back</span></a></div>
		<div class="C-header_right"><a class="button" href="#" onclick="this.blur();"><span>Design a Graphic</span></a></div>
		<h1>Add Graphics Content</h1>
	</div> 
	<div class="C-cont">
		<div class="C-center column">
			<%= render 'form' %>
		</div> 
		<div class="C-sidebar column"> 
			<%# content_for :left_sidebar do %>
			<ul>
			  <% content_types.each do |subclass| %>
			    <li<% if @content.class == subclass %> class="selected"<% end %>><%= link_to subclass.model_name.human, new_content_path(:type=>subclass.model_name.singular) %></li>
			  <% end %>
			</ul>
			<%# end %>
		</div>
		<div class="clear"></div>
	</div>
</div>
