<div id="SV-left" class="SV-offset">
	<ul>
		<% @feeds.each do |feed| %>
		<li><%= feed.name %> (<%= link_to 'Show', feed %>, <%= link_to 'Edit', edit_feed_path(feed) %>, <%= link_to 'Destroy', feed, :confirm => 'Are you sure?', :method => :delete %>)</li>
		<% end %>
	</ul>
</div>
<div id="SV-right" class="SV-offset">

	<table width="100%" cellpadding="0" cellspacing="0">
	  <tr>
	    <th>Name</th>
	    <th>Description</th>
	    <th>Parent</th>
	    <th>Group</th>
	  </tr>

	<% @feeds.each do |feed| %>
	  <tr>
	    <td><%= feed.name %></td>
	    <td><%= feed.description %></td>
	    <td><%= feed.parent_id %></td>
	    <td><%= feed.group.name unless feed.group.nil? %></td>
	  </tr>
	<% end %>
	</table>

	<br />

	<%= link_to 'New feed', new_feed_path %>
</div>
